function silverage_theme_suggestions_page_alter(array &$suggestions, array $variables, $hook) {
  $node = Drupal::request()->attributes->get('node');
  if ($node && $node->getType() == 'service') {
    $suggestions[] = 'page__service';

  }
  print_r($node->getType());
}